{
  "name": "prismawork",
  "private": true,
  "version": "1.0.0",
  "description": "A beautiful, UI-first tool for managing Prisma projects with multi-environment support",
  "keywords": ["prisma", "database", "studio", "ui", "typescript", "react"],
  "author": "PrismaWork Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wolfsonng/prismawork.git"
  },
  "bugs": {
    "url": "https://github.com/wolfsonng/prismawork/issues"
  },
  "homepage": "https://github.com/wolfsonng/prismawork#readme",
  "type": "module",
  "scripts": {
    "dev": "concurrently -k \"npm run server:dev\" \"npm run client:dev\"",
    "client:dev": "vite --host localhost --port 6581",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "server:dev": "npm --prefix server run dev",
    "server:start": "npm --prefix server run start",
    "client:preview": "vite preview --host localhost --port 6581",
    "build:all": "concurrently -k \"npm --prefix server run build\" \"vite build\"",
    "start": "concurrently -k \"npm run server:start\" \"npm run client:preview\""
  },
  "dependencies": {
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}